{
   "manifest_version": 2,

   "name": "iFrame Blocker",
   "version": "1.0",
   "description": "Block unwanted iframes",

   "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
  },
  
  

   "content_scripts" : [
   {
   "matches": ["<all_urls>"],
      "exclude_matches": ["*://*/_/chrome/newtab*"],
      "js": ["iFrameBlocker.js"]
   }
   
   ]
   
   
}


